{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "Fatal",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "NewRelicLogs",
        "Args": {
          "insertKey": "REPLACE_WITH_NEW_RELIC_KEY",
          "endpointUrl": "https://log-api.newrelic.com/log/v1",
          "applicationName": "ContactManagement"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  //"ConnectionStrings": {
  //  "ContactManagementDb": "Server=PREDATORNEO;Database=ContactManagementDb;Trusted_Connection=True;MultipleActiveResultSets=True;Application Name=Contact_Management_API"
  //},
  "Security": {
    "DefaultPassword": "REPLACE_WITH_SECRET_OR_USE_USER_SECRETS"
  },
  "Jwt": {
    "Key": "REPLACE_WITH_SECRET",
    "Issuer": "jwt",
    "Audience": "jwt",
    "ExpiryMinutes": 525600,
    "RefreshExpiryMinutes": 525600
  }
}
